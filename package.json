{
  "name": "@ewatercycle/jupyterlab_thredds",
  "version": "0.2.0",
  "description": "JupyterLab viewer for Thredds catalog",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/eWaterCycle/jupyterlab_thredds",
  "bugs": {
    "url": "https://github.com/eWaterCycle/jupyterlab_thredds/issues"
  },
  "license": "Apache-2.0",
  "author": "Stefan Verhoeven",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/eWaterCycle/jupyterlab_thredds.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "tslint --project tsconfig.json -t stylish"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/apputils": "^0.18.0",
    "@jupyterlab/docmanager": "^0.18.0",
    "@jupyterlab/docregistry": "^0.18.0",
    "@jupyterlab/filebrowser": "^0.18.0",
    "@jupyterlab/notebook": "^0.18.0",
    "@jupyterlab/services": "^3.0.0",
    "@phosphor/disposable": "^1.1.2",
    "react": "~16.4.0",
    "react-dom": "~16.4.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "~16.4.13",
    "@types/react-dom": "~16.0.7",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "~2.6.2"
  },
  "resolutions": {
    "@types/react": "~16.0.19"
  },
  "jupyterlab": {
    "extension": true
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "collectCoverage": true
  }
}
